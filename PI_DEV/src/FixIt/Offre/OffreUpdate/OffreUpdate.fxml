<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="rootPane" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FixIt.Offre.OffreUpdate.OffreUpdateController">
   <children>
      <AnchorPane id="AnchorPane" fx:id="mainContainer" prefHeight="376.0" prefWidth="490.0" style="-fx-background-color: #fff;" styleClass="custom-pane" stylesheets="@../../StyleSheet/dark-theme.css">
         <children>
            <VBox prefHeight="153.0" prefWidth="404.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <JFXTextField fx:id="titre" focusColor="#00a1ff" labelFloat="true" promptText="Titre" unFocusColor="#ffc107">
                     <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <ComboBox fx:id="categories" editable="true" focusTraversable="false" prefHeight="27.0" prefWidth="471.0" promptText="categorie">
                     <VBox.margin>
                        <Insets bottom="10.0" top="20.0" />
                     </VBox.margin>
                  </ComboBox>
                  <JFXTextField fx:id="budget" focusColor="#00a1ff" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="budget" unFocusColor="#ffc107">
                     <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <JFXTextField fx:id="desc" focusColor="#00a1ff" labelFloat="true" layoutX="10.0" layoutY="62.0" promptText="description" unFocusColor="#ffc107">
                     <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <HBox alignment="CENTER" prefHeight="47.0" prefWidth="404.0" spacing="30.0">
                     <children>
                        <JFXButton fx:id="saveButton" onAction="#AjouterOffre" prefHeight="66.0" prefWidth="100.0" style="-fx-background-color: #00a1ff;" text="Save" textFill="WHITE">
                           <font>
                              <Font name="Arial Rounded MT Bold" size="15.0" />
                           </font></JFXButton>
                        <JFXButton fx:id="cancelButton" layoutX="10.0" layoutY="10.0" onAction="#cancel" prefHeight="66.0" prefWidth="100.0" style="-fx-background-color: #ffc107;" text="Cancel" textFill="WHITE">
                           <font>
                              <Font name="Arial Rounded MT Bold" size="15.0" />
                           </font></JFXButton>
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
               <stylesheets>
                  <URL value="@../../StyleSheet/StyleSheet.css" />
                  <URL value="@../../StyleSheet/dark-theme.css" />
               </stylesheets>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
